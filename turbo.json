{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "lint",
        "^build"
      ],
      "outputs": [
        "website/.next/**",
        "content/data.json"
      ]
    },
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    },
    "lint": {}
  }
}
